{
  "manifest_version": 3,
  "name": "Speech Translator",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["aws-sdk-2.813.0.min.js", "content_script.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
